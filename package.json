{
  "name": "uniquener",
  "description": "A unique code generator by format",
  "homepage": "https://github.com/linpengteng/uniquener#readme",
  "version": "1.5.0",
  "license": "MIT",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "keywords": [
    "uuid",
    "uuid-format",
    "uuid-generator",
    "unique-generator",
    "unique-code"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.browser.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "shx rm -rf dist && rollup --config rollup.config.ts --configPlugin typescript",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linpengteng/uniquener.git"
  },
  "bugs": {
    "url": "https://github.com/linpengteng/uniquener/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/pluginutils": "^5.1.0",
    "@types/eslint": "^8.56.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.15",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "cz-message-helper": "^1.3.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.1",
    "prettier": "^3.2.4",
    "release-it": "^17.0.3",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "config": {
    "cz-message-helper": {
      "config": ".cz-message.cjs"
    },
    "commitizen": {
      "path": "node_modules/cz-message-helper"
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7"
  }
